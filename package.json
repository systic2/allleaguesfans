{
  "name": "allleaguesfans",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "typecheck": "tsc -b",
    "test": "vitest --run",
    "build": "pnpm typecheck && vite build",
    "preview": "vite preview",
    "deps:check": "depcheck --specials=typescript,binary",
    "exports:check": "ts-prune --ignore \"**/*.test.ts,**/*.test.tsx,**/*.spec.ts,**/*.spec.tsx\"",
    "files:check": "knip",
    "lint:unused": "eslint . --fix",
    "analyze:bundle": "ANALYZE=1 vite build",
    "sync:full": "npx tsx scripts/master-import.ts",
    "sync:daily": "npx tsx scripts/import-fixtures.ts && npx tsx scripts/import-standings.ts",
    "sync:weekly": "npx tsx scripts/import-squads.ts",
    "sync:check": "npx tsx scripts/final-verification.ts",
    "sync:monitor": "npx tsx scripts/monitoring.ts",
    "sync:debug": "npx tsx scripts/debug-api-response.ts",
    "sync:test-db": "npx tsx scripts/test-database.ts"
  },
  "dependencies": {
    "@supabase/supabase-js": "^2.56.1",
    "@tanstack/react-query": "^5.85.5",
    "lucide-react": "^0.542.0",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "react-router-dom": "^7.8.2"
  },
  "devDependencies": {
    "@eslint/js": "^9.34.0",
    "@tailwindcss/postcss": "^4.1.13",
    "@testing-library/jest-dom": "^6.8.0",
    "@testing-library/react": "^16.3.0",
    "@types/node": "^24.3.1",
    "@types/react": "^19.1.12",
    "@types/react-dom": "^19.1.9",
    "@vitejs/plugin-react-swc": "^3.7.1",
    "depcheck": "^1.4.7",
    "dotenv": "^17.2.2",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-unused-imports": "^3.2.0",
    "globals": "^16.3.0",
    "jsdom": "^26.1.0",
    "knip": "^5.30.0",
    "rollup-plugin-visualizer": "^5.12.0",
    "tailwindcss": "^4.1.12",
    "ts-prune": "^0.10.3",
    "typescript-eslint": "^8.42.0",
    "vite": "^7.1.4",
    "vitest": "^3.2.4"
  }
}
